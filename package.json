{
  "name": "time-tracker",
  "version": "1.0.0",
  "description": "Track how time is spent during the week",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cd backend && node server.js",
    "heroku-postbuild": "cd client && npm install && npm run build",
    "start:server": "cd backend && nodemon server.js --exec babel-node --presets es2015,stage-0",
    "start:client": "cd client && npm start",
    "start:dev": "concurrently \"npm run-script start:server\" \"npm run-script start:client\""
  },
  "author": "Sean Fox",
  "license": "ISC",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "concurrently": "^3.6.0",
    "express": "^4.16.3",
    "mongoose": "^5.2.1",
    "morgan": "^1.9.0",
    "nodemon": "^1.17.5"
  },
  "engines": {
    "node": "10.6.0"
  }
}
